<h1>Editing item</h1>

<% form_for(@item) do |f| %>
  <%= f.error_messages %>
	<%= render :partial => 'form_fields', :locals => {:f => f} %>
  <p>
    <%= f.label :rank_id %><br />
    <%= f.text_field :rank_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @item %> |
<%= link_to 'Back', items_path %>